
  package com.sathya.exchange;
  
  import jakarta.persistence.Column; import jakarta.persistence.Entity; import
  jakarta.persistence.GeneratedValue; import
  jakarta.persistence.GenerationType; import jakarta.persistence.Id; import
  jakarta.persistence.Table; import lombok.AllArgsConstructor; import
  lombok.Data; import lombok.NoArgsConstructor;
  
  @Data
  
  @NoArgsConstructor
  
  @AllArgsConstructor
  
  @Entity
  
  @Table(name = "currency_exchange") public class CurrencyExchange {
  
  @Id
  
  @GeneratedValue(strategy = GenerationType.AUTO) private Long id;
  
  @Column(name = "from_currency") private String fromCurrency;
  
  @Column(name = "to_currency") private String toCurrency;
  
  @Column(name = "conversion_rate") private Double conversionRate;
  
  
  }
  
 